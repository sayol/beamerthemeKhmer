\mode<presentation>

\setbeamercolor*{normal text}{fg=magenta,bg=white}
\setbeamercolor*{alerted text}{fg=blue}
\setbeamercolor*{example text}{fg=magenta}
\setbeamercolor*{structure}{fg=magenta}

\providecommand*{\beamer@bftext@only}{%
  \relax
  \ifmmode
    \expandafter\beamer@bftext@warning
  \else
    \expandafter\bfseries
  \fi
}
\providecommand*{\beamer@bftext@warning}{%
  \ClassWarning{beamer}
    {Cannot use bold for alerted text in math mode}%
}

\setbeamerfont{alerted text}{series=\beamer@bftext@only}

\setbeamercolor*{palette primary}{fg=white,bg=magenta!60}
\setbeamercolor*{palette secondary}{fg=white,bg=magenta!80}
\setbeamercolor*{palette tertiary}{fg=white,bg=magenta}
\setbeamercolor*{palette quaternary}{fg=white,bg=red}

\setbeamercolor{sidebar}{bg=magenta!20}

\setbeamercolor*{palette sidebar primary}{fg=magenta}
\setbeamercolor*{palette sidebar secondary}{fg=magenta}
\setbeamercolor*{palette sidebar tertiary}{fg=magenta}
\setbeamercolor*{palette sidebar quaternary}{fg=magenta}

\setbeamercolor*{item projected}{fg=white,bg=magenta}

\setbeamercolor*{titlelike}{parent=palette secondary}
\setbeamercolor*{block title}{fg=white,bg=magenta}
\setbeamercolor*{block title alerted}{fg=white,bg=magenta}
\setbeamercolor*{block title example}{fg=white,bg=magenta}

\setbeamercolor*{block body}{fg=magenta,bg=magenta!20}
\setbeamercolor*{block body alerted}{fg=magenta,bg=magenta!20}
\setbeamercolor*{block body example}{fg=magenta,bg=magenta!20}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode<all>
